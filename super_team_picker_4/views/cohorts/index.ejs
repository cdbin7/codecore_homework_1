<%-include('../partials/header.ejs') %> 


  <% for (let cohort of list) { %>
    <div class="card" style="margin: 20px">
      <div class="card-body">
    <img src="<%-cohort.logoUrl%>" alt="">
    <span class="card-title fs-3">
      <%-cohort.name %>
    </span>
    <p class="card-text fs-5">
      <%-cohort.member%>
    </p>
    <a class="btn btn-primary" href="/cohorts/<%-cohort.id%>">Show cohort</a>
    </div>
    <br>
    </div>
  <% } %> 







